{
	"manifest_version":1,
	"type":"WebApp",
	"meta":{
		"name":"GW2 TP Watcher",
		"version":"1.0.0",
		"minimum-overwolf-version":"0.77.10",
		"author":"David Dang and John Moon",
		"icon":"appIcon2.png",
		"icon_gray":"appIcon.png",
		"description":"Demo App"
	},
	"data": {
		"start_window":"MainWindow",
		"windows":{
			"MainWindow":{
					"file":"Files/index.html",
					"transparent":false,
					"resizable":true,
					"size":{"width":368, "height":340},
					"min_size":{"width":368, "height":340}
		
				},
			"SubWindow":{
					"file":"Files/searchWindow.html",
					"transparent":false,
					"resizable":true,
					"size":{"width":368, "height":368},
					"min_size":{"width":368, "height":368}
				}
		}
	}
}